{
  "name": "m-gov",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "pnpx rimraf ./**/node_modules ./pnpm-lock.yaml ./**/.next ./**/.turbo"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "next": "13.1.1",
    "prettier": "latest",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "pnpm@7.18.2"
}

{
  "name": "example-project",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build && pnpm cargo:make",
    "build:web": "turbo run build --filter=example-project-web",
    "build:docs": "turbo run build --filter=example-project-docs",
    "clean": "pnpx rimraf ./**/node_modules ./**/.next ./**/.turbo ./**/target ./**/.swc",
    "cargo:make": "cd server && cargo make v1",
    "cargo:run": "cd server && cargo run",
    "dev": "turbo run dev --parallel",
    "dev:web": "turbo run dev --filter=example-project-web",
    "dev:docs": "turbo run dev --filter=example-project-docs",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "dotenv-cli": "^6.0.0",
    "eslint-config-custom": "workspace:*",
    "next": "13.1.1",
    "prettier": "latest",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.18.2"
}
